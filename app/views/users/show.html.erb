
<div class="container" id="add-top-padding">
  <h1><%= @user.name %></h1>
  <p><%= @user.email %></p>
  <h5><%= "#{@user.name}'s Events" %></h5>
    <% @user.created_events.each do |event| %>
      <div class="row">
        <%= render 'layouts/event', :event => event  %>
      </div>
    <% end %>
</div>
